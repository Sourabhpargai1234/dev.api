{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/github",
        "dest": "/server.js"
      },
      {
        "src": "/api/github/topic",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }
  